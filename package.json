{
  "name": "theme-toggle-ext",
  "displayName": "Theme Toggle Extension",
  "publisher": "aasink",
  "description": "VS Code Extension that toggles the theme with a button on the bottom status bar.",
  "version": "1.0.0",
  "repository": {
		"type": "git",
		"url": "https://github.com/aasink/ThemeToggleExt"
	},
  "engines": { 
    "vscode": "^1.80.0" 
  },
  "main": "./src/extension.js",
  "activationEvents": [ 
    "onStartupFinished" 
  ],
  "contributes": { 
    "commands": [ 
      { 
        "command": "themeToggle.toggle", 
        "title": "Toggle Theme" 
      } 
    ] 
  },
  "dependencies": {}
}
