  "version": "1.0.0",
  "repository": {
		"type": "git",
		"url": "https://github.com/aasink/ThemeToggleExt"
	},
  "engines": { 
    "vscode": "^1.80.0" 
  },
  "main": "./src/extension.js",
  "contributes": { 
    "commands": [ 
      { 
        "command": "themeToggle.toggle", 
        "title": "Toggle Theme" 
      } 
    ] 
  },
  "dependencies": {
    "vscode": "^1.1.37"
  }
}
